{
  "name": "video-downloader-spa-renderer",
  "version": "1.1.42-SNAPSHOT",
  "description": "Renders SPA pages",
  "license": "MIT",
  "scripts": {
    "compile": "tsc",
    "compile:watch": "tsc -w",
    "clean": "rimraf build",
    "execute": "node build/src/main.js",
    "start": "npm run clean-compile && npm run execute",
    "clean-compile": "npm run clean && npm run compile",
    "test": "npm run clean && npm run jest",
    "jest": "jest --runInBand",
    "lint": "eslint src",
    "release": "node release/index.js"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "express": "^5.1.0",
    "puppeteer": "^24.8.2",
    "winston": "^3.17.0",
    "zod": "^3.25.32"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.27.0",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "rimraf": "^6.0.1",
    "simple-git": "^3.27.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  }
}
